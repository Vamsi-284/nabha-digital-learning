# API Configuration
REACT_APP_API_URL=https://api.nabha-learning.com
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_OFFLINE_MODE=true
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_DEBUG_MODE=false

# Localization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,hi,pa

# Storage Configuration
REACT_APP_MAX_OFFLINE_STORAGE=104857600
REACT_APP_SYNC_INTERVAL=300000

# Performance
REACT_APP_LAZY_LOADING=true
REACT_APP_IMAGE_OPTIMIZATION=true
